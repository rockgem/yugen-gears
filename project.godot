; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Yugen Gears"
run/main_scene="res://scenes/MainMenu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

ManagerGame="*res://scripts/ManagerGame.tscn"

[display]

window/stretch/mode="canvas_items"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[global_group]

Dot=""
Gear=""
DialDetector=""
Cannon=""

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="GearArea"
2d_physics/layer_2="DotArea"
2d_physics/layer_3="ClickableGearArea"
2d_physics/layer_4="DialDetectorArea"
2d_physics/layer_5="TargetArea"
2d_physics/layer_6="BulletArea"

[rendering]

renderer/rendering_method="mobile"
